          <Dependency module="ptc" path="/maybeEligibleForPtc" />
          <Dependency module="ctcOdc" path="/maybeEligibleForCtc" />
          <Dependency module="ctcOdc" path="/maybeEligibleForOdc" />
          <Dependency module="eitc" path="/maybeEligibleForEitc" />
